<table class="table table-dark table-striped">
    <tbody>
        <tr>
        <tr>
            <form action="/admin/create" method="post">
                <td>
                    <input type="name" name="name" id="name" placeholder="Name" class="form-control"
                        aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
                </td>
                <td>
                    <input type="email" name="email" id="email" placeholder="Email" class="form-control"
                        aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
                </td>
                <td>
                    <input type="text" name="username" id="username" placeholder="Username" class="form-control"
                        aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
                </td>
        </tr>
        <tr>
            <td>
                <button class="btn btn-outline-dark fw-bold text-light w-100" id="btn_cancel"
                    type="submit">CANCEL</button>
            </td>
            <td>
                <input type="password" name="password" id="password" placeholder="Password" class="form-control"
                    aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
            </td>
            <td>
                <button class="btn btn-outline-dark fw-bold text-light w-100" type="submit">CREATE</button>
            </td>
            </form>
        </tr>
        </tr>
    </tbody>
</table>
